<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:TwitchBot.MVVM.ViewModel"
                    xmlns:view="clr-namespace:TwitchBot.MVVM.View">

    <DataTemplate DataType="{x:Type TypeName=vm:StartBotViewModel}">
        <view:StartBotView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:CommandsViewModel}">
        <view:CommandsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:LoadingScreenViewModel}">
        <view:LoadingScreenView/>
    </DataTemplate>
    
</ResourceDictionary>